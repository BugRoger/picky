#!/usr/bin/env ruby
#

begin
  require 'picky'
rescue LoadError => e
  require 'rubygems'
  picky_path = File.expand_path('../../lib', __FILE__)
  $:.unshift(picky_path) if File.directory?(picky_path) && !$:.include?(picky_path)
  require 'picky'
end

generator = Picky::Generator.new
generator.run ARGV